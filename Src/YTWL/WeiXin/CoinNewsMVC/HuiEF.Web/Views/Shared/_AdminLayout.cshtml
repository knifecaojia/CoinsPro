<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>

    <link href="~/SiteFiles/bootstrap-v3/css/bootstrap.css" rel="stylesheet" />
    <link href="~/SiteFiles/bootstrap-table/bootstrap-table.css" rel="stylesheet" />
    <script src="~/SiteFiles/scripts/jquery-2.1.4.js"></script>
    <script src="~/SiteFiles/scripts/bootstrap.js"></script>
    <script src="~/SiteFiles/bootstrap-table/bootstrap-table.js"></script>
    <link href="~/SiteFiles/scripts/main.css" rel="stylesheet" />
  <style type="text/css">
      html, body { width:100%;height:100%}
  </style>
    @RenderSection("head", false)
</head>
<body>
    <div class="page-view">
        @RenderSection("header", false)
        <div class="view">
            @RenderBody()
        </div>
        @RenderSection("footer", false)
    </div>
    @RenderSection("body", false)
</body>
</html>
